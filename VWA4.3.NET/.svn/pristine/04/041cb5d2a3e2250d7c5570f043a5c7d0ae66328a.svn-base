using System;
using System.Collections.Generic;
using System.Text;
using VWA4Common.DAO;

namespace VWA4Common.DataObject
{
    public class UnitsVolume
    {
        private int id = 0;
        private string name = string.Empty;
        private string displayFullName = string.Empty;
        private string displayAbbreviatedName = string.Empty;
        private double conversionFactor = 0.00;
        private string description = string.Empty;

        public int Id
        {
            get { return this.id; }
            set { this.id = value; }
        }
        public string Name
        {
            get { return this.name; }
            set { this.name = value; }
        }
        public string DisplayFullName
        {
            get { return this.displayFullName; }
            set { this.displayFullName = value; }
        }
        public string DisplayAbbreviatedName
        {
            get { return this.displayAbbreviatedName; }
            set { this.displayAbbreviatedName = value; }
        }
        public double ConversionFactor
        {
            get { return this.conversionFactor; }
            set { this.conversionFactor = value; }
        }
        public string Description
        {
            get { return this.description; }
            set { this.description = value; }
        }
    }
}
